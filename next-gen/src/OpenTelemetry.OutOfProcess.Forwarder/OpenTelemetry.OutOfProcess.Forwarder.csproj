<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(TargetFrameworks)</TargetFrameworks>
    <OutputType>Library</OutputType>
    <RootNamespace>OpenTelemetry</RootNamespace>
    <PackageId>OpenTelemetry.OutOfProcess.Forwarder</PackageId>
    <Description>OpenTelemetry out-of-process forwarder library for collecting and forwarding telemetry data from applications to external collectors.</Description>
    <PackageTags>$(PackageTags);OutOfProcess;Forwarder</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" />
  </ItemGroup>

</Project>