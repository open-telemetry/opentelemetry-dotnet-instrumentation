{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:54564",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",

        "CORECLR_ENABLE_PROFILING": "1",
        "CORECLR_PROFILER": "{918728DD-259F-4A6A-AC2B-B85E1B658318}",
        "CORECLR_PROFILER_PATH": "$(ProjectDir)$(OutputPath)profiler-lib\\OpenTelemetry.ClrProfiler.Native.dll",

        "OTEL_DOTNET_TRACER_HOME": "$(ProjectDir)$(OutputPath)profiler-lib",
        "OTEL_INTEGRATIONS": "$(ProjectDir)$(OutputPath)profiler-lib\\integrations.json",
        "OTEL_VERSION": "1.0.0",
        "OTEL_TRACE_HEADER_TAGS": "sample.correlation.identifier, Server"
      },
      "use64Bit": true,
      "nativeDebugging": true
    },
    "Samples.AspNetCoreMvc31": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",

        "CORECLR_ENABLE_PROFILING": "1",
        "CORECLR_PROFILER": "{918728DD-259F-4A6A-AC2B-B85E1B658318}",
        "CORECLR_PROFILER_PATH": "$(ProjectDir)$(OutputPath)profiler-lib\\OpenTelemetry.ClrProfiler.Native.dll",

        "OTEL_DOTNET_TRACER_HOME": "$(ProjectDir)$(OutputPath)profiler-lib",
        "OTEL_INTEGRATIONS": "$(ProjectDir)$(OutputPath)profiler-lib\\integrations.json",
        "OTEL_VERSION": "1.0.0",
        "OTEL_SERVICE": "Samples.AspNetCoreMvc31",
        "OTEL_TRACE_HEADER_TAGS": "sample.correlation.identifier, Server",

        "OTEL_EXPORTER": "jaeger",
        "OTEL_EXPORTER_JAEGER_AGENT_HOST": "localhost",
        "OTEL_EXPORTER_JAEGER_AGENT_PORT": "6831"
      },
      "nativeDebugging": true,
      "applicationUrl": "http://localhost:54568"
    }
  }
}