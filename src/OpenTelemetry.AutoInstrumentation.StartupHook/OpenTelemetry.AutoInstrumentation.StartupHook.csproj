<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- To avoid application crash with .Net Core 3.1 and .NET 5.0 apps 
         this project should target netcoreapp3.1.
         Reference: https://github.com/open-telemetry/opentelemetry-dotnet-instrumentation/issues/1644 -->
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\OpenTelemetry.AutoInstrumentation\Constants.cs" Link="Constants.cs" />
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\CustomLogger.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\CustomLogger.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\FileSink.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\FileSink.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\IOtelLogger.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\IOtelLogger.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\ISink.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\ISink.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\LogLevel.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\LogLevel.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\NoopSink.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\NoopSink.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\OtelLogging.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\OtelLogging.cs</Link>
    </Compile>
    <Compile Include="..\Shared\OpenTelemetry.AutoInstrumentation.Logging\WriteCountingStream.cs">
      <Link>OpenTelemetry.AutoInstrumentation.Logging\WriteCountingStream.cs</Link>
    </Compile>
  </ItemGroup>

</Project>
