{
  "name": "opentelemetry-dotnet-instrumentation",
  "version": "0.0.1",
  "description": "Application to configure and execute markdownlint and spellcheck documentation.",
  "scripts": {
    "markdownlint": "npx markdownlint . -i node_modules",
